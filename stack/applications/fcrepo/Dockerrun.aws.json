{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "fcrepo-app",
      "image": "nulib/fcrepo4:s3fix",
      "essential": true,
      "memory": 3000,
      "portMappings": [
        { "hostPort": 80,   "containerPort": 8080 },
        { "hostPort": 8080, "containerPort": 8080 }
      ],
      "environment": [
        {
          "name": "MODESHAPE_CONFIG",
          "value": "classpath:/config/jdbc-postgresql/repository.json"
        }
      ]
    }
  ]
}
